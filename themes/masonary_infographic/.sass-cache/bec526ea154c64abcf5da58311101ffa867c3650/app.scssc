/* Primary Styles
 Author: Matt Watier
 */
.evencolumns{overflow:hidden;
	> .column{padding-bottom:500px;margin-bottom:-500px;
	}
}

h1, h2, h3, h4 , strong,p.large{
	@include display-font();
	letter-spacing:1px;
}

h5,h6,p{
	
	font-family:'SansationRegular', "trebuchet MS", Arial, sans-serif;
}
	
	p.small,blockquote{
	
	font-family:'SansationLight', "trebuchet MS", Arial, sans-serif;
	color:#222
}
p{ letter-spacing: 0.07em;
    line-height: 1.75em;
    margin: 0 0 1.5em;}
p.large{color:#444}

#main{
	#introduction{
		h1{
			@include light-font();
			font-size: 4.5em;
		}
		h4{
			@include copy-font();
			color: $mainColor;
		}
		strong{
			color: $mainColor;
		}

	}
	#galleries{
		.gallery{
			padding:0 25px;
			h2{
				@include copy-font();
				font-size: 31px;
				margin-bottom: -15px;
				margin-left: -25px;
				letter-spacing: 3px;
			}
			.d3_chart{
				margin-left: -25px;
			}
			P{ color: #888;line-height: 1.35em;}
			.thumbnails{
				list-style: none;
				padding:0;
				margin-left: -25px;
				.thumbnail{
					float:left; 
					margin:2px;
					a{
						display: inline-block;
						border: 1px solid $secondaryColor;
						padding:2px;
						&:hover{
							border: 1px solid $mainColor;
						}
						img{
							border: 0px none #fff;vertical-align: middle;
						}
					}
				}
			}
		}
	}
}

	

header{
	&#siteheader {
	position: fixed;
	border-top: solid #333 5px;
	width: 100%;
	z-index: 3000;
	}
	.wrap{
		background-color:#bfbfbf;
		display:inline-block;
		border-radius: 0px 0px 10px 10px;
		-moz-border-radius: 0px 0px 10px 10px;
		-webkit-border-bottom-right-radius: 10px;
		-webkit-border-bottom-left-radius: 10px;
	}
	nav {
		@include display-font();
	    color: #222;
		float:left;
		height:32px;
		padding:0px 20px 3px;
		
		&.siteLinks{
		background-color: #333333;
		border-radius: 0px 0px 10px 10px;
		-moz-border-radius: 0px 0px 10px 10px;
		-webkit-border-bottom-right-radius: 10px;
		-webkit-border-bottom-left-radius: 10px;
		}
		&.socialLinks{
			background-color: #bfbfbf;
			border-radius: 0px 0px 10px 0px;
		-moz-border-radius: 0px 0px 10px 0px;
		-webkit-border-bottom-right-radius: 10px;
			
		}
		a {
		    color: #BFBFBF;
		    display: block;
		    float: left;
		    font-size: 105%;
		    height: 20px;
		    padding-top: 8px;
		    padding-left:15px;
		    text-decoration: none;
		&.logo + a {margin-left:15px;}  
		  
		}
		
	}

}	


a{
	&.icon{
		padding:0px;
		height:32px;
		width:40px;
		background: url(icon-sprites.png) no-repeat top left;
		text-indent: -2000px;
		overflow: hidden;
	
	}
	&.logo {
			padding-left: 20px;
			width: 92px;
			background: url(icon-sprites.png) transparent no-repeat top left;
	  }
	
}



a.logo{background-position: left top;}
a.facebook{background-position:-40px top;}
a.twitter{background-position:-80px top;}
a.linkedin{background-position:-120px top;}
a.contact{background-position:0px top;}
a.deviantart{background-position:-160px top;}

a:hover.logo{background-position: left bottom;}
a:hover.facebook{background-position:-40px bottom; }
a:hover.twitter{background-position:-80px bottom; }
a:hover.linkedin{background-position:-120px bottom; }
a:hover.contact{background-position:0 bottom; }
a:hover.deviantart{background-position:-160px bottom; }




/* mobile */
a.icon,a.logo,span.icon{
	display:block;
	float:left;
	padding:0px;
	margin:0px;
	width:32px;
	height:32px;
	background: url(icon-sprites.png) no-repeat top left;
	text-indent: -2000px;
	overflow: hidden;

}
a.logo{
	background-color: #333333;
	background-position:0 -64px;
	width:64px;
	border-radius: 0px 0px 10px 10px;
	-moz-border-radius: 0px 0px 10px 10px;
	-webkit-border-bottom-radius:0px 0px  10px 10px;
	padding:0px 3px 3px;
	margin-bottom: -3px;
	}
div#siteheaderbottom a.comment{
	border-radius: 10px 10px 0px 0px;
	-moz-border-radius: 10px 10px 0px 0px;
	-webkit-border-bottom-radius:10px 10px 0px 0px;
    background-color: #333333;
    margin-left: 0;
    margin-top: 0;
    padding: 3px 1px 0;
    width: 64px;

	
}
a.facebook{background-position:-32px top;}
a.comment{background-position:0px bottom;}

a.twitter{background-position:-64px top;}
a.linkedin{background-position:-96px top;}
a.contact{background-position:0px top;}
a.deviantart{background-position:-128px top;}
a.skipPrev{background-position:-192px 0px;}
a.skipNext{background-position:-192px -32px;}
a.forward{background-position:-160px -32px;}
a.cancel{background-position:-160px -64px;overflow: visible; text-decoration: none;padding:7px 2px;}
div#siteheadertop.ui-header a.cancel{
	text-indent: 32px;color:white;font-weight: normal;
	
}

a:hover.logo,a:active.logo{background-position: -64px -64px;}
a:hover.comment,a:active.comment{background-position: -64px bottom;}

a:hover.facebook,a:active.facebook{background-position:-32px -32px; }
a:hover.twitter,a:active.twitter{background-position:-64px -32px; }
a:hover.linkedin,a:active.linkedin{background-position:-96px -32px; }
a:hover.contact,a:active.contact{background-position:0 -32px; }
a:hover.deviantart,a:active.deviantart{background-position:-128px -32px; }
a:hover.forward,a:active.forward{background-position:-160px -32px;}
a:hover.skipPrev,a:active.skipPrev{background-position:-224px 0px;}
a:hover.skipNext,a:active.skipNext{background-position:-224px -32px;}
a:hover.cancel,a:active.cancel{background-position:-192px -64px;}

#canvas{
    padding-top: 100px;
    }
    
    /**** Isotope filtering ****/
    
    .isotope-item {
      z-index: 2;
    }
    
    .isotope-hidden.isotope-item {
      pointer-events: none;
      z-index: 1;
    }
    

/*Base Design*/
@media all and (orientation:portrait) { 
  
}

@media all and (orientation:landscape) { 
  
}



@media screen and (max-width: 1260px) and (min-width: 900px) {
	#main{
		 #introduction{
		 
		 	@include column(8);
		 }
		 #galleries{
		 	@include column(8);
		 	.gallery{
		 		@include column(16);
		 		.thumbnails{
		 			@include column(6);
		 		}
		 		P{
		 			@include column(10);
		 		}
		 	}
		 }
	}
	#dataholder{
		margin-left: -200px;
	}

}
@media screen and (max-width: 900px) {
	#main{
		 #introduction{
		 	h1{font-size: 3.5em;}
		 	@include column(10);
		 }
		 #galleries{
		 	@include column(6);
		 	.gallery{
		 	.d3_chart{
		 	margin-left: -35px;
		 	}
		 		@include column(16);
		 		.thumbnails{
		 				@include column(16);
		 				margin-left: 0;
		 		}
		 		 p{
		 				@include column(16);
		 				padding: 0;
		 		}
		 	}
		 }
	}
	#dataholder{display: none;}
 
}
