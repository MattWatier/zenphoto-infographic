@mixin display-font(){
	font-family:'SansationBold', "trebuchet MS", Arial, sans-serif;
	font-weight: 900;
}

@mixin copy-font(){
	font-family:'SansationRegular', "trebuchet MS", Arial, sans-serif;
	font-weight: 300;
}


@mixin light-font(){
	font-family:'SansationLight', "trebuchet MS", Arial, sans-serif;
	font-weight: 100;
}


